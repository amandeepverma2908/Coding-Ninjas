import java.util.Arrays;
public class solution {

	public static String uniqueChar(String str){
		// Write your code here
        String s=new String();
        int l=str.length();
        for(int i=0;i<l;i++)
        {
            char c=str.charAt(i);
            if(s.indexOf(c)<0)
            {
                s+=c;
            }
        }
        return s;
    }
}
